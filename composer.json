{
    "name": "gamegos/util",
    "description": "Miscellaneous utility classes",
    "keywords": [
        "utilities"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.5.0"
    },
    "autoload": {
        "psr-4": {
            "Gamegos\\Util\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.8 - 5.7",
        "gamegos/php-code-sniffer": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Gamegos\\Util\\Tests\\": "tests/"
        }
    },
    "config": {
        "cache-dir": ".tmp/composer"
    },
    "authors": [
        {
            "name": "Safak Ozpinar",
            "email": "safak@gamegos.com"
        }
    ]
}
